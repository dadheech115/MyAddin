    <Hosts>
      <Host xsi:type="MailHost">
        <DesktopFormFactor>

          <ExtensionPoint xsi:type="MessageReadCommandSurface">
            <OfficeTab id="TabDefault">
              <Group id="msgReadTabMessage.grp1">

                <Label resid="residAppName" />
                <Tooltip resid="residAppDescription" />

                <Control xsi:type="Button" id="TaskPane0BtnRead">
                  <Label resid="residTaskPaneButton0Name" />
                  <Tooltip resid="residTaskPaneButton0Name" />
                  <Supertip>
                    <Title resid="residTaskPaneButton0Name" />
                    <Description resid="residReadSupertip" />
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="tp16icon" />
                    <bt:Image size="32" resid="tp32icon" />
                    <bt:Image size="80" resid="tp80icon" />
                  </Icon>
                  <Action xsi:type="ShowTaskpane">
                    <SourceLocation resid="residTaskpaneUrl" />
                  </Action>
                </Control>

              </Group>
            </OfficeTab>
          </ExtensionPoint>

          <ExtensionPoint xsi:type="MessageComposeCommandSurface">
            <OfficeTab id="TabDefault">
              <Group id="msgComposeTabMessage.grp1">

                <Label resid="residAppName" />
                <Tooltip resid="residAppDescription" />

                <Control xsi:type="Button" id="TaskPane0BtnCompose">
                  <Label resid="residTaskPaneButton0Name" />
                  <Tooltip resid="residTaskPaneButton0Name" />
                  <Supertip>
                    <Title resid="residTaskPaneButton0Name" />
                    <Description resid="residComposeSupertip" />
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="tp16icon" />
                    <bt:Image size="32" resid="tp32icon" />
                    <bt:Image size="80" resid="tp80icon" />
                  </Icon>
                  <Action xsi:type="ShowTaskpane">
                    <SourceLocation resid="residTaskpaneComposeUrl" />
                  </Action>
                </Control>

              </Group>
            </OfficeTab>
          </ExtensionPoint>

        </DesktopFormFactor>
      </Host>
    </Hosts>

